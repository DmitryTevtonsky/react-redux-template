{
  "name": "space_web_vr",
  "version": "1.0.0",
  "private": true,
  "description": "VR project",
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@reduxjs/toolkit": "^1.5.1",
    "antd": "^4.15.1",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "i18next": "^19.7.0",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.0.18",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.2",
    "react-redux": "^7.2.3",
    "react-scripts": "^3.4.3",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3"
  },
  "scripts": {
    "clean": "rimraf src/api/impl dist",
    "generate-translations": "i18next",
    "codegen": "yarn clean && yarn download-swagger && yarn generate-swagger-client-swvr",
    "generate-graphql": "node libs/download-schema.js && yarn graphql-codegen --config codegen.yml",
    "generate-swagger-client-swvr": "openapi-generator generate --skip-validate-spec -i swagger_swvr.json -g typescript-axios -o src/api/impl/swvr",
    "download-swagger": "node libs/download-swagger.js",
    "analyze": "yarn build --analyze",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "format:prettier": "prettier --write \"src/**/*.{ts,tsx}\" package.json",
    "format:stylelint": "stylelint --color --fix \"src/**/*.css\"",
    "format:eslint": "eslint --fix --cache \"src/**/*.{ts,tsx}\"",
    "format": "yarn format:prettier && yarn format:eslint && yarn format:stylelint",
    "test": "yarn codegen && yarn test:unit",
    "test:unit": "react-app-rewired test --env=jsdom --coverage --reporters=default --reporters=jest-junit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@graphql-codegen/cli": "^1.17.0",
    "@graphql-codegen/typescript": "^1.17.0",
    "@graphql-codegen/typescript-operations": "^1.17.0",
    "@openapitools/openapi-generator-cli": "^1.0.18-5.0.0-beta2",
    "@types/classnames": "^2.2.9",
    "@types/express": "^4.17.2",
    "@types/node": "^13.7.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^6.1.0",
    "@types/sockjs-client": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "apollo-server": "^2.15.1",
    "babel-plugin-import": "^1.13.0",
    "copy-webpack-plugin": "^6.1.0",
    "customize-cra": "^1.0.0",
    "dotenv-flow": "^3.1.0",
    "download": "^8.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-deprecate": "^0.7.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jest-dom": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "eslint-plugin-testing-library": "^3.8.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "http-proxy-middleware": "^1.0.5",
    "husky": "^4.2.2",
    "i18next-localstorage-backend": "^3.1.2",
    "i18next-parser": "^3.3.0",
    "less-loader": "^8.0.0",
    "lint-staged": "^10.0.7",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.1.0",
    "react-app-rewire-unplug": "^0.9.0",
    "react-app-rewired": "^2.1.5",
    "stylelint": "^13.6.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-property-sort-order-smacss": "^7.1.0",
    "stylelint-config-recommended": "^3.0.0",
    "typed-css-modules-webpack-plugin": "^0.2.0",
    "typescript": "^4.0.2",
    "typescript-plugin-css-modules": "^2.4.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
